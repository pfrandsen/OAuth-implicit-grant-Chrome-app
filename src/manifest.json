{
  "name": "__MSG_appName__",
  "version": "0.6",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "default_locale": "en",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": ["browser", "system.network", "webview", "storage"],
  "webview": {
    "partitions": [
      {
        "name": "trusted",
        "accessible_resources": ["init.html", "img/oauth2.png"]
      }
    ]
  },
  "sockets": {
    "tcpServer": {
      "listen": [
        "*"
      ]
    }
  }
}